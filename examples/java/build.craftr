project "examples.java"

import {glob} from 'craftr.craftr'
import 'java.craftr'

target "lib":
  java.srcs = glob('src/lib/*.java')
  export java.artifacts = ['org.tensorflow:tensorflow:1.4.0']

target "main":
  requires "@lib"
  java.srcs = ['src/Main.java']
  java.mainClass = 'Main'
  java.bundleType = 'merge'
