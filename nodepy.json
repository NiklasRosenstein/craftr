{
  "name": "craftr",
  "version": "3.0.0-dev",
  "description": "Craftr is a domain-independent build system.",
  "license": "MIT",
  "main": "./lib/public",
  "resolve_root": "./lib",
  "bin": {
    "craftr": "./lib/main"
  },
  "authors": [
    "rosensteinniklas@gmail.com"
  ],
  "pip_dependencies": {
    "glob2": ">=0.6",
    "Werkzeug": ">=0.12.2",
    "toml": ">=0.9.3",
    "colorama": ">=0.3.9",
    "termcolor": ">=1.1.0",
    "nr": ">=1.4.8"
  },
  "scripts": {
    "test": "$ nodepy-nosetests lib/utils",
    "docs": "$ yassg public"
  },
  "cfg(dev).dependencies": {
    "nodepy-nosetests": "git+https://github.com/nodepy/nodepy-nosetests.git@v0.0.6",
    "@NiklasRosenstein/yassg": "git+https://github.com/NiklasRosenstein/yassg.git@v0.2.0"
  }
}